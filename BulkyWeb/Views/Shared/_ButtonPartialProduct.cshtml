@model int
@{
    ViewData["Title"] = "_ButtonPartialProduct";
}


<td style="width:150px">
    <div class="btn-group">
        <a asp-action="Upsert" asp-controller="Product" class="btn btn-success mx-2" asp-route-id="@Model"><i class="bi bi-pencil-square"></i></a>
        <a asp-action="Delete" asp-controller="Product" class="btn btn-danger" asp-route-id="@Model"><i class="bi bi-trash"></i></a>
    </div>
